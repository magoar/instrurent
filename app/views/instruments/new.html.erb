<div class="page-container">
  <div class="card-instrument p-5">
    <h1>New Instrument</h1>
    <%= simple_form_for(@instrument) do |f| %>
      <%= f.input :name %>
      <%= f.input :instrument_type %>
      <%= f.input :description %>
      <%= f.input :price_per_day %>
      <%= f.input :photo, as: :file %>
      <%= f.submit class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
