<div class="container m-3">
  <h1>New Instrument</h1>
  <%= simple_form_for(@instrument) do |f| %>
    <%= f.input :name %>
    <%= f.input :instrument_type %>
    <%= f.input :description %>
    <%= f.input :price_per_day %>
    <%= f.input :photo, as: :file %>
    <%= f.submit %>
  <% end %>
</div>
